<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;CA1031;NU1701</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="akavache" Version="6.9.1" />
    <PackageReference Include="DynamicData" Version="6.13.13" />
    <PackageReference Include="MaterialDesignColors" Version="1.2.1-ci643" />
    <PackageReference Include="MaterialDesignExtensions" Version="2.8.0-a01" />
    <PackageReference Include="MaterialDesignThemes" Version="3.0.0-ci643" />
    <PackageReference Include="ReactiveUI" Version="10.4.1" />
    <PackageReference Include="ReactiveUI.Events.WPF" Version="10.4.1" />
    <PackageReference Include="ReactiveUI.Fody" Version="10.4.1" />
    <PackageReference Include="ReactiveUI.Validation" Version="1.4.1" />
    <PackageReference Include="ReactiveUI.WPF" Version="10.4.1" />
    <PackageReference Include="Serilog" Version="2.9.0-dev-01133" />
    <PackageReference Include="Serilog.Sinks.Debug" Version="1.0.2-dev-00020" />
    <PackageReference Include="Serilog.Sinks.File" Version="4.1.0-dev-00850" />
    <PackageReference Include="Splat" Version="9.1.1" />
    <PackageReference Include="Splat.Serilog" Version="9.1.1" />
    <PackageReference Include="System.Reactive" Version="4.2.0-preview.625" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MovieList.Data\MovieList.Data.csproj" />
    <ProjectReference Include="..\MovieList.ViewModels\MovieList.ViewModels.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Messages.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Messages.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Messages.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Messages.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
